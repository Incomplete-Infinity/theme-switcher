{
  "source": {
		"include": ["./src/"],
		"exclude": ["*"],
		"includePattern": ".+\\.(js|md)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown", "plugins/summarize"],
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	},
	"templates": {
    "name": "Doc Template",
		"footerText": "NHN Entertainment Frontend Development Lab",
		"logo": {
			"link": "https://github.com/nhnent/tui.jsdoc-template"
	},
		"tabNames": {
			"api": "API",
			"tutorials": "Examples"
		}
	},
	"opts": {
		"encoding": "utf8",
		"recurse": true,
		"package": "package.json",
		"template": "node_modules/tui-jsdoc-template"
	}
}
