{
  "source": {
    "include": ["index.html, script.js, style.css"],
    "exclude": ["*"],
    "includePattern": ".+\\.(js|md)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "name": "Theme Switcher",
    "footerText": "Copyright 2023 Andrew Amason",
    "logo": {
      "link": "https://github.com/nhnent/tui.jsdoc-template"
    },
    "tabNames": {
      "api": "API",
      "tutorials": "Examples"
    }
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "package": "package.json",
    "template": "node_modules/tui-jsdoc-template"
  }
}
